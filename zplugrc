source ~/.zplug/init.zsh
zplug "zplug/zplug"

zplug "dekz/prompt", use:prompt.zsh

zplug "junegunn/fzf-bin", \
    from:gh-r, \
    as:command, \
    rename-to:fzf, \
    use:"*darwin*amd64*"

zplug "stedolan/jq", \
    from:gh-r, \
    as:command, \
    rename-to:jq

zplug "zsh-users/zsh-syntax-highlighting", nice:1
zplug "supercrabtree/k"
zplug "rupa/z", use:z.sh

if ! zplug check --verbose; then
  printf "Install? [y/N]: "
  if read -q; then
      echo; zplug install
  fi
fi

zplug load
